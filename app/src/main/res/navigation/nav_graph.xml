<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/newsList2">
    <fragment
        android:id="@+id/newsList2"
        android:name="com.example.newsapp.newsList.NewsList">
        <action
            android:id="@+id/action_newsList2_to_newsInfoFragment2"
            app:destination="@id/newsInfoFragment2" />
        <action
            android:id="@+id/action_newsList2_to_newsFavFragment"
            app:destination="@id/newsFavFragment" />
    </fragment>
    <fragment
        android:id="@+id/newsInfoFragment2"
        android:name="com.example.newsapp.newsInfo.NewsInfoFragment">
        <argument
            android:name="articles"
            app:argType="com.example.newsapp.newsNetwork.Articles" />
    </fragment>
    <fragment
        android:id="@+id/newsFavFragment"
        android:name="com.example.newsapp.newsFavorite.NewsFavFragment"
        android:label="NewsFavFragment" />
</navigation>